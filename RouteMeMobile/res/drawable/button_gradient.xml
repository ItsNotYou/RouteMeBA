<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape>
            <gradient
                android:angle="270"
                android:centerColor="#FFE3F1FA"
                android:endColor="#FFD7EBF9"
                android:startColor="#FFD7EBF9"
                android:type="linear" />

            <stroke
                android:width="1dp"
                android:color="#FFD7EBF9" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:centerColor="#00666666"
                android:centerX="0.5"
                android:centerY="0"
                android:endColor="#77666666"
                android:gradientRadius="200"
                android:startColor="#00666666"
                android:type="radial" />

            <stroke
                android:width="1dp"
                android:color="#FF000000" />
        </shape>
    </item>

</layer-list>